generator kysely {
    provider     = "prisma-kysely"
    output       = ".."
    enumFileName = "enums.ts"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

enum Foo {
    BAR
    BAZ
}

model Post {
    id      String @id
    userId  String
    title   String
    content String

    @@index([userId])
}

model Customer {
    id       String @id
    name     String
    email    String
    foo      Foo
    stripeId String
}
